    "require 'fasttext'\n",
    "\n",
    "# documents\n",
    "x = [\n",
    "  \"text from document one\",\n",
    "  \"text from document two\",\n",
    "  \"text from document three\"\n",
    "]\n",
    "\n",
    "# labels\n",
    "y = [\"ham\", \"ham\", \"spam\"]\n",
    "\n",
    "model = FastText::Classifier.new\n",
    "model.fit(x, y)\n",
    "\n",
    "# puts model.predict(x)\n",
    "\n",
    "puts model.words\n",
    "puts model.labels"
   ]
